{
    "name": "neugen/helpers",
    "description": "Helpers",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/FI-LIFE/helpers",
    "authors": [
        {
            "name": "Evgeniy Naumenko",
            "email": "justfilife@gmail.com",
            "homepage": "https://neugen.ru/"
        }
    ],
    "keywords": [
        "helpers"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "ext-readline": "*",
        "ext-mbstring": "*"
    },
    "suggest": {
        "ext-readline": "to use Helpers\\Cli"
    },
    "autoload": {
        "psr-4": {
            "Neugen\\Helpers\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Neugen\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.14"
    }
}
